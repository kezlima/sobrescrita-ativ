#encapsulamento 

'''class funcionario():



    

    def __init__(self,nome,idade):

        self._nome=nome

        self._idade=idade


    @property

    def nome(self):

        return self._nome


    @nome.setter

    def nome(self,nome):

        self._nome=nome


    @property

    def idade (self):

        return self.__idade


    @idade.setter

    def idade(self,idade):

     self.__idade=idade


pessoa=funcionario(input("nome:"), input("idade:"))
print(pessoa._nome)
print(pessoa._idade)'''
   

